{
    "name": "konceiver/laravel-passwordless",
    "description": "Convert bytes to a human readable strings",
    "keywords": [
        "konceiver",
        "laravel-passwordless"
    ],
    "homepage": "https://github.com/konceiver/laravel-passwordless",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@konceiver.dev",
            "homepage": "https://konceiver.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/validation": "^8.0",
        "konceiver/passphrase": "^1.1",
        "laravel/fortify": "^1.7",
        "nesbot/carbon": "^2.41",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "konceiver/laravel-stan": "^1.1",
        "konceiver/stan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Konceiver\\Passwordless\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konceiver\\Passwordless\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --coverage --min=100"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
